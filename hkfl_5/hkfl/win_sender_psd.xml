$sys_set.send_pack_flag = 1;
hmi_window_hide("win_sender_psd");
strcpy($picture_ctrl.win_wait_tip,"后台处理中，请耐心等待");
$picture_ctrl.win_wait_sw = 0;
//$sys_set.user_ic_enable_flag = 1;
$$HmiCommit;
hmi_window_show("win_wait_tip");
]]></event>
